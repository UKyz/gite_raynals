<script>
    let demoPicker = new Datepickk({
        container: document.querySelector('#demoPicker'),
        inline: true,
        range: true
    });
    demoPicker.minDate = new Date(moment().subtract(1, 'days'));
    demoPicker.lang = 'fr';
    demoPicker.disabledDays = [1,2,3,4,5,0];

    demoPicker.onSelect = (checked) => {
        if (demoPicker.selectedDates.length > 0) {
            const date_debut = moment(demoPicker.selectedDates[0]).format('DD/MM/YYYY');
            const date_fin =
                moment(demoPicker.selectedDates[demoPicker.selectedDates.length - 1]).format('DD/MM/YYYY');
            document.getElementById('date_begin').value = date_debut;
            document.getElementById('date_end').value = date_fin;

            const url = './?action=updatePriceResa';
            let dat = { 'services_choice' : []};
            $("input:checked").each(function() {
                dat['services_choice'].push($(this).val());
            });
            $.post(url, { date_begin: document.getElementById('date_begin').value,
                date_end: document.getElementById('date_end').value, choices: dat }).done(function(data){
                $('input[id="calcul_price"]').val(data);
            });
            const url2 = './?action=updateDetailPrice';
            $.post(url2, { date_begin: document.getElementById('date_begin').value,
                date_end: document.getElementById('date_end').value, choices: dat }).done(function(data){
                $('textarea[id="detail_price"]').val(data);
            });
        }
    };

    {$calendrier_script}
</script>
